<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Disable Library Validation for Python executable -->
	<!-- This allows Python to load libraries with different Team IDs or unsigned libraries -->
	<!-- This is necessary for dynamically installed Python packages -->
	<key>com.apple.security.cs.disable-library-validation</key>
	<true/>
</dict>
</plist>

